{
  "name": "mern_shopping_list",
  "version": "1.0.0",
  "engines": {
    "node": "10.14.2"
  },
  "description": "SHopping list built with MERN stak",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd clientNPM_CONFIG_PRODUCTION=false npm install && npm run build"
  },
  "author": "Jeusc",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "mongoose": "^4.13.17"
  },
  "devDependencies": {
    "nodemon": "^1.18.8"
  }
}
